{"ast":null,"code":"import { PictureFilled, PictureRounded } from \"@element-plus/icons-vue\";\nimport MovieCard from \"@/components/basic/MovieCard\";\nimport SvgIcon from \"@/components/basic/SvgIcon\";\nexport default {\n  name: \"MovieList\",\n  components: {\n    SvgIcon,\n    PictureRounded,\n    PictureFilled,\n    MovieCard\n  },\n  props: {\n    tag: {\n      type: String,\n      required: true,\n      default: '我的推荐'\n    },\n    movies: {\n      type: Object\n    },\n    opt: {\n      /**\r\n       * 0：默认\r\n       * 1：猜你喜欢\r\n       * 2：最多人看\r\n       * 3：评分最高\r\n       */\n      type: Number,\n      required: true,\n      default: 0\n    }\n  },\n  setup(props) {\n    props.movies.forEach((movie, idx) => {\n      if (movie.idx) {\n        movie.idx *= 100;\n        // 保留两位小数（四舍五入）\n        movie.idx = movie.idx.toFixed(2);\n      }\n    });\n  }\n};","map":{"version":3,"mappings":"AA6DA,SAAQA,aAAa,EAAEC,cAAc,QAAO,yBAAyB;AACrE,OAAOC,SAAQ,MAAO,8BAA8B;AACpD,OAAOC,OAAM,MAAO,4BAA4B;AAEhD,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAACF,OAAO;IAAEF,cAAc;IAAED,aAAa;IAAEE;EAAS,CAAC;EAC/DI,KAAK,EAAE;IACLC,GAAG,EAAE;MACHC,IAAI,EAAEC,MAAM;MACZC,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE;IACX,CAAC;IACDC,MAAM,EAAE;MACNJ,IAAI,EAAEK;IACR,CAAC;IACDC,GAAG,EAAE;MACH;;;;;;MAMAN,IAAI,EAAEO,MAAM;MACZL,QAAQ,EAAE,IAAI;MACdC,OAAO,EAAE;IACX;EACF,CAAC;EACDK,KAAKA,CAACV,KAAK,EAAE;IACXA,KAAK,CAACM,MAAM,CAACK,OAAO,CAAC,CAACC,KAAK,EAAEC,GAAG,KAAK;MACnC,IAAID,KAAK,CAACC,GAAG,EAAE;QACbD,KAAK,CAACC,GAAE,IAAK,GAAE;QACf;QACAD,KAAK,CAACC,GAAE,GAAID,KAAK,CAACC,GAAG,CAACC,OAAO,CAAC,CAAC;MACjC;IACF,CAAC;EACH;AACF","names":["PictureFilled","PictureRounded","MovieCard","SvgIcon","name","components","props","tag","type","String","required","default","movies","Object","opt","Number","setup","forEach","movie","idx","toFixed"],"sourceRoot":"","sources":["F:\\personalSpace\\lx-bishe\\movie_recommendation_system_vue\\src\\components\\home\\MovieList.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <p class=\"panel-title\">\r\n      <svg-icon v-if=\"opt === 1\" icon-class=\"recommendation_5\"></svg-icon>\r\n      <svg-icon v-if=\"opt === 2\" icon-class=\"watched\"></svg-icon>\r\n      <svg-icon v-if=\"opt === 3\" icon-class=\"score_1\" style=\"color: rgb(247,186,42)\"></svg-icon>\r\n      {{ tag }}......\r\n    </p>\r\n\r\n    <el-scrollbar>\r\n      <div class=\"scrollbar-flex-content\">\r\n        <template v-for=\"(movie, index) in movies\" :key=\"movie.id\">\r\n          <movie-card :movie=\"movie\" class=\"movie-card-item\">\r\n            <!-- 猜你喜欢 -->\r\n            <template v-if=\"opt === 1\" v-slot:header>\r\n              <svg-icon v-if=\"index === 0\" icon-class=\"recommendation_1\" class=\"recommendation-svg-first\"\r\n                        style=\"color: rgb(255,215,0)\"></svg-icon>\r\n              <svg-icon v-if=\"index === 1\" icon-class=\"recommendation_1\" class=\"recommendation-svg-first\"\r\n                        style=\"color: rgb(241,241,235)\"></svg-icon>\r\n              <svg-icon v-if=\"index === 2\" icon-class=\"recommendation_1\" class=\"recommendation-svg-first\"\r\n                        style=\"color: rgb(186,110,64)\"></svg-icon>\r\n              <svg-icon v-if=\"index > 2\" icon-class=\"recommendation\" class=\"recommendation-svg-second\"\r\n                        style=\"color: rgb(48,208,186)\"></svg-icon>\r\n            </template>\r\n            <!-- 最多人看 -->\r\n            <template v-if=\"opt === 2\" v-slot:header>\r\n              <svg-icon v-if=\"index === 0\" icon-class=\"hot\" class=\"hot-svg\" style=\"color: rgb(238,63,77)\"></svg-icon>\r\n              <svg-icon v-if=\"index === 1\" icon-class=\"hot\" class=\"hot-svg\" style=\"color: rgb(250,126,35)\"></svg-icon>\r\n              <svg-icon v-if=\"index === 2\" icon-class=\"hot\" class=\"hot-svg\" style=\"color: rgb(226,216,73)\"></svg-icon>\r\n              <svg-icon v-if=\"index > 2\" icon-class=\"hot\" class=\"hot-svg\" style=\"color: rgb(49,112,167)\"></svg-icon>\r\n            </template>\r\n            <!-- 评分最高 -->\r\n            <template v-if=\"opt === 3\" v-slot:header>\r\n              <svg-icon v-if=\"index === 0\" icon-class=\"first\" class=\"highest-score-svg-first\"></svg-icon>\r\n              <svg-icon v-if=\"index === 1\" icon-class=\"second\" class=\"highest-score-svg-first\"></svg-icon>\r\n              <svg-icon v-if=\"index === 2\" icon-class=\"third\" class=\"highest-score-svg-first\"></svg-icon>\r\n              <svg-icon v-if=\"index > 2\" icon-class=\"recommendation_3\" class=\"highest-score-svg-first\"\r\n                        style=\"color: rgb(48,208,186)\"></svg-icon>\r\n            </template>\r\n\r\n            <template v-if=\"opt === 1\" v-slot:footer>\r\n              <div class=\"recommendation-index-div\">\r\n                <svg-icon icon-class=\"recommendation_5\"></svg-icon>\r\n                推荐指数: {{ movie.idx }}%\r\n              </div>\r\n            </template>\r\n\r\n            <template v-if=\"opt === 2\" v-slot:footer>\r\n              <div class=\"watched-num-div\">\r\n                <svg-icon icon-class=\"watched\"></svg-icon>\r\n                {{ movie.num }}人评价\r\n              </div>\r\n            </template>\r\n          </movie-card>\r\n        </template>\r\n      </div>\r\n    </el-scrollbar>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {PictureFilled, PictureRounded} from \"@element-plus/icons-vue\";\r\nimport MovieCard from \"@/components/basic/MovieCard\";\r\nimport SvgIcon from \"@/components/basic/SvgIcon\";\r\n\r\nexport default {\r\n  name: \"MovieList\",\r\n  components: {SvgIcon, PictureRounded, PictureFilled, MovieCard},\r\n  props: {\r\n    tag: {\r\n      type: String,\r\n      required: true,\r\n      default: '我的推荐'\r\n    },\r\n    movies: {\r\n      type: Object,\r\n    },\r\n    opt: {\r\n      /**\r\n       * 0：默认\r\n       * 1：猜你喜欢\r\n       * 2：最多人看\r\n       * 3：评分最高\r\n       */\r\n      type: Number,\r\n      required: true,\r\n      default: 0,\r\n    }\r\n  },\r\n  setup(props) {\r\n    props.movies.forEach((movie, idx) => {\r\n      if (movie.idx) {\r\n        movie.idx *= 100\r\n        // 保留两位小数（四舍五入）\r\n        movie.idx = movie.idx.toFixed(2)\r\n      }\r\n    })\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.panel-title {\r\n  font-size: 26px;\r\n  color: #e52a0d;\r\n  font-weight: bold;\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.scrollbar-flex-content {\r\n  display: flex;\r\n  flex-wrap: nowrap;\r\n}\r\n\r\n.movie-card-item {\r\n  margin: 1rem;\r\n}\r\n\r\n.recommendation-svg-first {\r\n  position: absolute;\r\n  top: -3px;\r\n  left: -3px;\r\n  z-index: 10;\r\n  width: 2rem;\r\n  height: 2rem;\r\n}\r\n\r\n.recommendation-svg-second {\r\n  position: absolute;\r\n  top: -3px;\r\n  left: -3px;\r\n  z-index: 10;\r\n  width: 3rem;\r\n  height: 3rem;\r\n}\r\n\r\n.recommendation-index-div {\r\n  text-align: center;\r\n  margin-bottom: 1rem;\r\n  color: rgb(250, 114, 104);\r\n}\r\n\r\n.highest-score-svg-first {\r\n  position: absolute;\r\n  z-index: 10;\r\n  width: 2rem;\r\n  height: 2rem;\r\n}\r\n\r\n.watched-num-div {\r\n  text-align: center;\r\n  margin-bottom: 1rem;\r\n  color: rgb(125, 197, 235);\r\n}\r\n\r\n.hot-svg {\r\n  position: absolute;\r\n  z-index: 10;\r\n  left: -1px;\r\n  top: -2px;\r\n  width: 2rem;\r\n  height: 2rem;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}